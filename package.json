  "description": "Testing library for Superface capabilities.",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "author": "Superface Team",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
